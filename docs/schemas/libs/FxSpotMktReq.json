{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FxSpotMktReq",
  "type": "object",
  "required": [
    "settle_calender",
    "spot_lag",
    "trading_calender"
  ],
  "properties": {
    "settle_calender": {
      "$ref": "#/definitions/CalendarSym"
    },
    "spot_lag": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "trading_calender": {
      "$ref": "#/definitions/CalendarSym"
    }
  },
  "definitions": {
    "CalendarSym": {
      "description": "A symbol for a calendar. Combined calendars are available. As an atom, only alphanumeric characters or '_' are allowed.",
      "examples": [
        "TK",
        "TK|NY",
        "TK&NY",
        "(TK|NY)&(LN|TK)"
      ],
      "type": "string"
    }
  }
}